<h1><center> Login </center></h1>
<%= form_for :session, url: login_path do |f| %>
	<p>    
    <%= f.label :name%>
    <%= f.text_field :name, class: 'form-control' %>
  </p>   
        
  <p> 
    <%= f.label :email%>
    <%= f.text_field :email, class: 'form-control' %>
  </p>
    
  <p>
    <%= f.submit%>
  </p>
<%end%>

